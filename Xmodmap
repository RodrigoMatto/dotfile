!	0x0f1
!keysym  ntilde = Control_L l
!keysym  ntilde = Caps_Lock
!keycode  47 = ntilde 

!keycode  57 = ntilde 
!	WORKS
!keysym  n = ntilde 
!add ntildekey = 93

!keycode 93 = Alt_L n

